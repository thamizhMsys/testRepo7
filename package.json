{
  "name": "msys-github-dashboard-api",
  "version": "1.0.0",
  "description": "middleware application for Github",
  "main": "server.js",
  "scripts": {
    "eslint": "node_modules\\.bin\\eslint .; exit $?",
    "eslint-fix": "node_modules\\.bin\\eslint --fix .;",
    "eslint:github-action": "eslint .",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --fix --ext .ts src",
    "lint:report": "eslint --ext .ts --output-file eslint_report.json --format json src",
    "start": "node server.js .env",
    "dev": "nodemon server.js .env",
    "prettier-auto-fix": "prettier --check --write './**/*.js' --ignore-path=.prettierignore",
    "test": "mocha ./test/*.test.js --timeout 10000000",
    "coverage": "nyc --reporter html --reporter text npm test"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "swagger/*",
    "test/*",
    "coverage/*",
    "helpers/logger.js",
    "gateway/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint"
    }
  },
  "author": "Msys Technologies",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.25.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.18.2",
    "core-util-is": "^1.0.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.2",
    "fs": "0.0.1-security",
    "helmet": "^5.0.2",
    "jira-connector": "^3.1.0",
    "jsonwebtoken": "^8.1.1",
    "moment": "^2.29.1",
    "mongoose": "^5.10.0",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.7.3",
    "nodemon": "^2.0.15",
    "octonode": "^0.9.5",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.10.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.1.18",
    "babel-eslint": "^8.2.1",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^4.3.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "istanbul": "^0.4.5",
    "mkdirp": "^0.5.1",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1"
  },
  "babel": {
    "presets": [
      "env",
      "es2015",
      "stage-0",
      "latest"
    ]
  }
}
